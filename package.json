{
  "name": "tcp-chat",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cache:update": "./bin/deno cache -r --unstable --lock=deno-lock.json --lock-write src/start-app.js",
    "start": "./bin/deno run --allow-net --allow-env --allow-read --unstable --lock=deno-lock.json src/start-app.js",
    "start:dev": "nodemon --exec './bin/deno run --allow-net --allow-env --allow-read --unstable src/start-app.js'"
  },
  "engines": {
    "node": "14"
  },
  "license": "MIT",
  "dependencies": {
    "ansi-escapes": "^4.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
